<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5527A4"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/main_activity"
        android:layout_width="202dp"
        android:layout_height="202dp"
        app:srcCompat="@drawable/maijn_activity"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="74dp" />

    <ImageView
        android:id="@+id/activity_main"
        android:layout_width="202dp"
        android:layout_height="202dp"
        app:srcCompat="@drawable/activity_main"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="74dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="202dp"
        android:layout_height="202dp"
        app:srcCompat="@drawable/pantalla"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="276dp" />

    <ImageView
        android:id="@+id/andorid_manifest"
        android:layout_width="202dp"
        android:layout_height="202dp"
        app:srcCompat="@drawable/manifest"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="276dp" />

    <TextView
        android:id="@+id/kotlin_para_diseñadores"
        android:layout_width="263dp"
        android:layout_height="32dp"
        android:fontFamily="sans-serif-medium"
        android:text="Kotlin para diseñadores"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="38dp"
        android:layout_height="41dp"
        app:srcCompat="@drawable/x"
        tools:layout_editor_absoluteX="369dp"
        tools:layout_editor_absoluteY="19dp" />

    <ImageView
        android:id="@+id/constructor"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/constructor"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="478dp" />

    <ImageView
        android:id="@+id/metodo"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/metodo"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="478dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="41dp"
        android:layout_height="41dp"
        app:srcCompat="@drawable/logo_kotlin"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="19dp" />

    <ImageView
        android:id="@+id/instancia"
        android:layout_width="120dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/instanmcia"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="478dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="657dp" />

    <EditText
        android:id="@+id/textView2"
        android:layout_width="350dp"
        android:layout_height="147dp"
        android:hint="haz tu búsqueda"
        android:text="TextView"
        android:textColor="#FFFCFC"
        android:textColorHighlight="#F6E5E5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="718dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="352dp"
        android:layout_height="41dp"
        android:background="#8E62D8"
        android:hint="haz tu búsqueda"
        android:textColor="#FFFCFC"
        android:textColorHighlight="#F6E5E5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="601dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="409dp"
        android:layout_height="72dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp" />

</androidx.constraintlayout.widget.ConstraintLayout>